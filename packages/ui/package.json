{
  "name": "@adara-cs/ui-kit-web",
  "version": "2.6.4",
  "description": "AdaraCloud UI Kit for Web",
  "license": "MIT",
  "keywords": [
    "ui-kit",
    "ui",
    "ts",
    "typescript",
    "js"
  ],
  "author": "adara-cs",
  "bugs": {
    "url": "https://github.com/adara-cs/ui-kit-web/issues"
  },
  "homepage": "https://github.com/adara-cs/ui-kit-web#readme",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./style.css": "./dist/index.css",
    "./components": {
      "source": "./src/components/index.ts",
      "import": {
        "types": "./dist/components/index.d.ts",
        "default": "./dist/components/index.js"
      }
    },
    "./*": {
      "source": "./src/components/*/index.ts",
      "import": {
        "types": "./dist/components/*/index.d.ts",
        "default": "./dist/components/*/index.js"
      }
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "check-types": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@adara-cs/hooks": "workspace:*",
    "@adara-cs/types": "workspace:*",
    "@adara-cs/utils": "workspace:*",
    "@adara-cs/icons": "workspace:*",
    "@floating-ui/react": "^0.27.5",
    "@maskito/core": "^3.4.0",
    "@maskito/phone": "^3.4.0",
    "@maskito/react": "^3.4.0",
    "@maskito/kit": "^3.4.0",
    "libphonenumber-js": "^1.11.19",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-remove-scroll": "^2.6.3"
  },
  "devDependencies": {
    "@testing-library/react": "^16.1.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "rollup-preserve-directives": "^1.1.3",
    "vite": "^6.0.12",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.0.0"
}
