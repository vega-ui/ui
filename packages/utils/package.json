{
  "name": "@vega-ui/utils",
  "version": "1.0.1",
  "description": "Utilities for @vega-ui/react",
  "license": "MIT",
  "keywords": [
    "ui-kit",
    "ui",
    "ts",
    "typescript",
    "js"
  ],
  "author": "VegaUI Team",
  "bugs": {
    "url": "https://github.com/vega-ui/ui/issues"
  },
  "homepage": "https://github.com/vega-ui/ui#readme",
  "type": "module",
  "main": "./dist/index.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest --watch"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "vite": "^6.0.13",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.0.0"
}
